{
	"name": "aimeos/ai-symfony2",
	"description": "Aimeos extension for utilizing Symfony 2 components",
	"homepage": "http://aimeos.org",
	"type": "arcavias-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-symfony2",
		"issues": "https://github.com/aimeos/ai-symfony2/issues"
	},
	"require": {
		"zendframework/zend-servicemanager": "2.3.*",
		"zendframework/zend-i18n": "2.3.*",
		"swiftmailer/swiftmailer": "5.*",
		"symfony/symfony": "2.*",
		"monolog/monolog": "1.*",
		"php": ">=5.3.2"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}