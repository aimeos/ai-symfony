{
	"name": "aimeos/ai-symfony2",
	"description": "Symfony 2 adapter for Aimeos web shops and e-commerce solutions",
	"keywords": ["aimeos", "adapter", "symfony", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/Symfony",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-symfony2",
		"issues": "https://github.com/aimeos/ai-symfony2/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"symfony/symfony": "2.*",
		"composer/installers": "~1.0",
		"php": ">=5.3.2"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
