{
	"name": "aimeos/ai-symfony",
	"description": "Symfony 2 adapter for Aimeos web shops and e-commerce solutions",
	"keywords": ["aimeos", "adapter", "symfony", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/Symfony",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-symfony",
		"issues": "https://github.com/aimeos/ai-symfony/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "^1.0",
		"symfony/symfony": "~2.4||~3.0",
		"zendframework/zend-diactoros": "~1.0",
		"aimeos/aimeos-core": "~2017.04"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MW\\": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
