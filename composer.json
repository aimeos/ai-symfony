{
	"name": "aimeos/ai-symfony",
	"description": "Symfony adapter for Aimeos e-commerce integration",
	"keywords": ["aimeos", "adapter", "symfony", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/Symfony",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-symfony",
		"issues": "https://github.com/aimeos/ai-symfony/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1||~8.0",
		"nyholm/psr7": "~1.2",
		"symfony/framework-bundle": "~6.2",
		"aimeos/aimeos-core": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	}
}
