{
	"name": "aimeos/ai-symfony2",
	"description": "Aimeos extension for utilizing Symfony 2 components",
	"keywords": ["aimeos", "adapter", "symfony"],
	"homepage": "http://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-symfony2",
		"issues": "https://github.com/aimeos/ai-symfony2/issues",
		"forum": "https://help.aimeos.org",
		"wiki": "https://docs.aimeos.org"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"symfony/symfony": "2.*",
		"php": ">=5.3.2"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
